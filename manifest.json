{
	"name": "Analyse Me",
	"version": "1.4",
	"background_page": "background.html",
	"content_scripts": [
    {
		"all_frames": true,
      "js": ["contentscript.js"],
      "matches": ["http://*/*","https://*/*"],
		"run_at": "document_end"
    }
  ],
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"permissions": [
		"tabs"
	]
}
